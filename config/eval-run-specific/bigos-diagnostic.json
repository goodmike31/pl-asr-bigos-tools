{
    "name":"BIGOS",
    "eval_run_codename": "alpha",
    "datasets": ["amu-cai/pl-asr-bigos-v2-diagnostic"],
    "subsets": ["pjatk-clarin_mobile-15", "pjatk-clarin_studio-15", "mailabs-corpus_librivox-19", "pwr-azon_read-20", "pwr-azon_spont-20", "pwr-maleset-unk", "pwr-viu-unk", "pwr-shortwords-unk", "google-fleurs-22", "polyai-minds14-21"],
    "splits": ["test", "validation", "train"],
    "max_samples_per_subset_to_evaluate": 200,
    "eval_metrics": ["lexical"],
    "ref_types": ["orig", "written", "spoken"],
    "norm_types": ["none", "all", "lowercase", "blanks", "punct", "dict"],
    "sampling_settings_for_manual_inspection": {"random": 3},
    "norm_types_for_manual_inspection": ["all"],
    "metadata_for_manual_inspection": ["all"],
    "systems": 
    {
            
        "assembly_ai":{
            "models": ["nano", "best"],
            "versions": ["2024Q1"]
            },
        
        "google": { 
            "models": ["default", "command_and_search", "latest_long", "latest_short"],
            "versions": ["2024Q1"]
            },
        
        "google_v2": { 
            "models": ["long", "short"],
            "versions": ["2024Q1"]
            },

        "azure":{ 
            "models": ["latest"],
            "versions": ["2024Q1"]
            },

        "nemo":{
            "models": ["stt_pl_fastconformer_hybrid_large_pc", "stt_pl_quartznet15x5", "stt_multilingual_fastconformer_hybrid_large_pc"],
            "versions": ["2024Q1"]
            },

        "wav2vec2":{ 
            "models": ["large-xlsr-53-polish", "xls-r-1b-polish"],
            "versions": ["2024Q1"]
            },

        "mms":{ 
            "models": ["1b-all", "1b-fl102", "1b-l1107"],
            "versions": ["2024Q1"]
            },

        "whisper_cloud":{ 
            "models": ["whisper-1"],
            "versions": ["2024Q1"]
            },
            
        "whisper_local":{ 
            "models": ["tiny", "base", "small", "medium", "large", "large-v1", "large-v2"],
            "versions": ["2024Q1"]
            }

        }
}
